{
  "version": 2,
  "installCommand": "cd backend && npm install",
  "buildCommand": "cd backend && npm run build || echo 'No build step'",
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/server.js"
    }
  ]
}